<!DOCTYPE html>
<html>
<head>
  <title>Sketchstitchstart</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <div>
<% if designer_signed_in? %>
    Welcome <%=  current_designer.email %>
    <%= link_to "Sign out", destroy_designer_session_path, :method => :delete %>
<% else %>
    <%= link_to "Sign in", new_designer_session_path%> or <%= link_to "Sign up", new_designer_registration_path %>
<% end %>
</div>

<%= yield %>

</body>
</html>
